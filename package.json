{
    "name": "@liquid-js/qr-code-styling",
    "version": "4.0.8",
    "description": "Generate styled QR codes on web or in Node",
    "type": "module",
    "main": "./lib/qr-code-styling-node.js",
    "browser": "./lib/qr-code-styling.js",
    "types": "./lib/index.d.ts",
    "exports": {
        "./package.json": {
            "default": "./package.json"
        },
        ".": {
            "node": "./lib/qr-code-styling-node.js",
            "types": "./lib/index.d.ts",
            "default": "./lib/qr-code-styling.js"
        },
        "./kanji": {
            "types": "./lib/kanji.d.ts",
            "default": "./lib/kanji.js"
        }
    },
    "files": [
        "lib"
    ],
    "peerDependencies": {
        "@liquid-js/qrcode-generator": "^1.0.2",
        "@xmldom/xmldom": "^0.9.0",
        "file-type": "^19.0.0",
        "sharp": ">0.32.1 <1.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@liquid-js/qrcode-generator": "^1.1.1",
        "@liquid-js/rollup-plugin-closure-compiler": "^0.28.0",
        "@rollup/plugin-commonjs": "^28.0.6",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.4",
        "@stylistic/eslint-plugin": "^5.3.1",
        "@testdeck/mocha": "^0.3.3",
        "@types/chai": "^5.2.2",
        "@types/jsdom": "^21.1.7",
        "@types/jsdom-global": "^3.0.7",
        "@types/mocha": "^10",
        "@types/node": "<25.0.0",
        "@types/sharp": "^0.32.0",
        "@types/source-map-support": "^0.5.10",
        "@xmldom/xmldom": "^0.9.6",
        "canvas": "^3.2.0",
        "chai": "^6.0.1",
        "clean-package": "^2.2.0",
        "commit-and-tag-version": "^12.6.0",
        "eslint": "^9.34.0",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsdoc": "^54.2.1",
        "eslint-plugin-prefer-arrow-functions": "^3.6.2",
        "eslint-plugin-unused-imports": "^4.2.0",
        "file-type": "^21.0.0",
        "google-closure-compiler": "^20250820.0.0",
        "husky": "^9.1.7",
        "jsdom": "^26.1.0",
        "jsdom-global": "^3.0.2",
        "mocha": "^11.7.2",
        "npm-check-updates": "^18.0.3",
        "rimraf": "^6.0.1",
        "rollup": "^4.50.0",
        "rollup-plugin-minify-template-literals": "^1.1.7",
        "rollup-plugin-replace-regex": "^1.1.0",
        "sharp": "^0.34.3",
        "source-map-support": "^0.5.21",
        "ts-node-maintained": "^10.9.6",
        "typedoc": "^0.28.12",
        "typescript": "^5.9.2",
        "typescript-eslint": "^8.42.0",
        "vite": "^7.1.4"
    },
    "scripts": {
        "build": "rimraf lib && rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript",
        "test": "mocha -p",
        "start": "vite src",
        "lint": "eslint . --fix",
        "prepublishOnly": "npm run build",
        "prepare": "husky",
        "release": "git diff --exit-code && commit-and-tag-version && git push --follow-tags origin",
        "docs": "typedoc",
        "prepack": "clean-package",
        "postpack": "clean-package restore"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Liquid-JS/qr-code-styling.git"
    },
    "keywords": [
        "qr",
        "qrcode",
        "qr-code",
        "js",
        "qrjs",
        "qrstyling",
        "styling",
        "qrbranding",
        "branding",
        "qrimage",
        "image",
        "qrlogo",
        "logo",
        "design",
        "node",
        "nodejs"
    ],
    "author": "HitkoDev <development@hitko.si>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Liquid-JS/qr-code-styling/issues"
    },
    "homepage": "https://styled-qr.liquidjs.io/",
    "packageManager": "yarn@4.9.4",
    "clean-package": {
        "remove": [
            "packageManager",
            "scripts",
            "devDependencies",
            "clean-package"
        ]
    }
}
